<?xml version='1.0'?>
<robot name="pool">

<link name="world" />
<joint name="world_joint"  type="fixed">
    <origin xyz="32 -27 -40" rpy="0 0 0" />
    <parent link="world"/>
    <child link="pool_base_link"/>
</joint>

  <!-- Base Link -->
  <link name="pool_base_link">

    <!-- Caster -->
    <visual name="pool">
      <origin xyz="0.2 0 -0.125" rpy="0 0 0" />
      <geometry>
        <mesh><uri>file://pool.obj</uri></mesh>
      </geometry>
    </visual>
    <!-- Caster collision, mass and inertia -->
    <collision>
      <origin xyz="0.2 0 -0.125" rpy="0 0 0" />
      <geometry>
        <mesh><uri>file://pool.obj</uri></mesh>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
    </inertial>
    <gazebo>
      <static>true</static>
    </gazebo>

  </link>

</robot>
